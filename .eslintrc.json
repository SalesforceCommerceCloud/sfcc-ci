{
  "parserOptions": {
    "ecmaVersion": 8
  },

  "rules": {
    "brace-style": ["error", "1tbs"],
    "keyword-spacing": "error",
    "no-multi-spaces": "error",
    "max-len": ["error", 120, {"ignoreComments": true}],
    "valid-jsdoc": "off",
    "no-trailing-spaces": "error",
    "indent": ["error", 4]
  }
}